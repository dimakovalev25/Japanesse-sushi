..')\n            }\n            const data = await response.json();\n\n            const loadedJokes = [];\n            for(const key in data) {\n                loadedJokes.push({\n                    id: key,\n                    setup: data[key].setup,\n                    punchline: data[key].punchline,\n                })\n            }\n\n            // setJokes(data);\n            setJokes(loadedJokes);\n        } catch (e) {\n            setError(e.message)\n        }\n        setIsLoading(false);\n\n    }, [])\n\n    useEffect(() => {\n        fetchJokesHandler()\n    }, [fetchJokesHandler])\n\n    const addJokeHandler = async function (newJoke) {\n\n        // console.log(e)\n        // setJokes(() => {\n        //    return [ newJoke, ...jokes ]\n        // })\n\n        const response =  await fetch('https://react-course-86712-default-rtdb.europe-west1.firebasedatabase.app/jokes.json',{\n            method: 'POST',\n            body: JSON.stringify(newJoke),\n            headers: {\n                'Content-Type': 'application/json'\n            }\n        });\n        const data = await response.json();\n        console.log(data)\n\n\n    }\n\n    return (\n        <React.Fragment>\n            <section>\n                <button onClick={fetchJokesHandler}>Fetch Jokes</button>\n            </section>\n            <section>\n                <AddJoke onAddJoke={addJokeHandler}/>\n            </section>\n            <section>\n                {jokes !== null && jokes !== undefined && !isLoading && jokes.length > 0 && <JokeList jokes={jokes}/>}\n                {!isLoading && jokes.length === 0 && !error && <p>Jokes is not find</p>}\n                {isLoading && <p>Loading...</p>}\n                {!isLoading && error && <p>{error}</p>}\n            </section>\n\n        </React.Fragment>\n    );\n}\n\nexport default AppHttp;\n","D:\\dima\\YA_React_useStyle\\src\\Http\\JokeList.js",[],"D:\\dima\\YA_React_useStyle\\src\\Http\\Joke.js",[],"D:\\dima\\YA_Reac